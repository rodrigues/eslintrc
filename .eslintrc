{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "blockBindings": true,
      "defaultParams": true,
      "templateStrings": true,
      "jsx": true
    }
  },
  "plugins": [
    "babel",
    "jest",
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "15.3"
    }
  },
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": [2, "1tbs", {"allowSingleLine": true}],
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "computed-property-spacing": 2,
    "curly": [2, "multi-line"],
    "default-case": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "func-call-spacing": 2,
    "func-style": [2, "declaration", {"allowArrowFunctions": true}],
    "generator-star-spacing": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "key-spacing": [2, {"mode": "minimum", "beforeColon": false}],
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "max-depth": 2,
    "max-len": [2, {"code": 99, "ignoreUrls": true, "ignoreComments": true, "ignoreTrailingComments": true, "ignoreTemplateLiterals": true, "ignoreStrings": true}],
    "max-nested-callbacks": 2,
    "max-params": 2,
    "max-statements-per-line": [2, {"max": 2}],
    "new-cap": [2, {"capIsNew": false}],
    "no-cond-assign": [2, "always"],
    "no-console": [2, {"allow": ["error"]}],
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": [2, {"exceptions": {"Property": true}}],
    "no-multiple-empty-lines": [2, {"max": 1, "maxBOF": 0, "maxEOF": 1}],
    "no-nested-ternary": 2,
    "no-self-assign": 2,
    "no-sequences": 2,
    "no-trailing-spaces": 2,
    "no-unreachable": 2,
    "no-unused-expressions": [2, {"allowShortCircuit": true, "allowTernary": true, "allowTaggedTemplates": true}],
    "no-unused-vars": 2,
    "no-var": 2,
    "no-with": 2,
    "object-curly-newline": [2, {"multiline": true, "consistent": true, "minProperties": 8}],
    "object-shorthand": 2,
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": [2, "after"],
    "padded-blocks": [2, "never"],
    "padding-line-between-statements": [
      2,
      {"blankLine": "always", "prev": ["return", "if", "switch"], "next": "*"},
      {"blankLine": "any", "prev": "if", "next": "if"},
      {"blankLine": "always", "prev": "multiline-block-like", "next": "*"},
      {"blankLine": "always", "prev": "*", "next": "multiline-block-like"},
      {"blankLine": "always", "prev": "const", "next": "const"}
    ],
    "prefer-arrow-callback": 2,
    "prefer-const": [2, {"destructuring": "all"}],
    "prefer-destructuring": 2,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single"],
    "semi-spacing": 2,
    "semi": [2, "never"],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": 2,
    "strict": [2, "never"],
    "switch-colon-spacing": 2,
    "template-tag-spacing": 2,
    "use-isnan": 2,
    "yoda": 2,

    "babel/object-curly-spacing": 2,

    "jest/no-disabled-tests": 2,
    "jest/no-focused-tests": 2,
    "jest/valid-expect": 2,

    "react/jsx-boolean-value": 2,
    "react/jsx-closing-tag-location": 2,
    "react/jsx-closing-bracket-location": [2, {"selfClosing": "after-props", "nonEmpty": "after-props"}],
    "react/jsx-curly-spacing": 2,
    "react/jsx-equals-spacing": 2,
    "react/jsx-first-prop-new-line": 2,
    "react/jsx-key": 2,
    "react/jsx-max-props-per-line": [2, {"when": "multiline"}],
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-tag-spacing": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-wrap-multilines": 2,
    "react/no-direct-mutation-state": 2,
    "react/react-in-jsx-scope": 2
  }
}
